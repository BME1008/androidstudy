<!--4-->
    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_toLeftOf="@id/five"
        android:layout_centerVertical="true"
        android:background="#2196F3"/>
<!--5-->
    <TextView
        android:id="@+id/five"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true"
