    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#FF0000"/>

    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#FF0000"/>

    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#FF0000"/>

    <TextView
        android:id="@+id/view1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#FF0000"/>


</RelativeLayout>